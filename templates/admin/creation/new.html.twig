{% import "macros/table.html.twig" as table %}
{% import "macros/head.html.twig" as head %}
{% import "macros/foot.html.twig" as foot %}
{% import "macros/breadcrumb.html.twig" as utils %}
{% import "macros/alertes.html.twig" as alerte %}
{% import "macros/button.html.twig" as btn %}
{% import "macros/link.html.twig" as link %}

{% extends 'admin/base.html.twig' %}


{% block stylesheets %}
  {{ head.dataTable() }}
{% endblock %}

{% block breadcrumb %}
    {{ utils.breadcrumb('Manage creation',[
        {
            path:path('creation_index'),
            name:'Manage creation'
        }
    ])}}
{% endblock %}
{% block title %}New Creation{% endblock %}

{% block body %}
<div class="row">
    <div class="col-lg-12">
        <div class="card card-outline card-primary">
            <div class="card-header">
                <h4>Create new Creation</h4>
            </div>
            <div class="card-body">
                  {{ include('admin/creation/_form.html.twig') }}
            </div>
            <div class="card-footer">
                    {{ link.back(path('creation_index')) }}
            </div>
        </div>
    </div>
</div>
{% endblock %}
